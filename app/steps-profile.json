[
  {
    "id": 1,
    "title": "Contacto",
    "description": "Tus datos son necesarios para poder manetener comunicación acertiva con tu paciente y mindmeet, recuerda que podras modificarlos en la sección <br/><strong>Tu perfíl</strong>",
    "fields": [
      {
        "label": "Nombre publico",
        "name": "contacto.publicName",
        "type": "text",
        "placeholder": "Psic.Wilhelm Wundt",
        "rules": {
          "required": true
        }
      },
      {
        "name": "contacto.telefono",
        "label": "Teléfono",
        "type": "tel",
        "placeholder": "5512345678",
        "rules": {
          "required": true,
          "minLength": 10,
          "maxLength": 10
        }
      },
      {
        "label": "Whatsapp",
        "name": "contacto.whatsapp",
        "type": "tel",
        "placeholder": "5512345678",
        "rules": {
          "required": true,
          "minLength": 10,
          "maxLength": 10
        }
      }
    ]
  },
  {
    "id": 2,
    "title": "Especialidades",
    "fields": [
      {
        "name": "educacion.especialidades",
        "label": "Especialidades",
        "type": "select",
        "multiple": true,
        "options": [
          {
            "label": "Ansiedad",
